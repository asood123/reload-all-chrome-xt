{
  "name": "Reload All Tabs",
  "version": "1.1",
  "description": "Reloads all tabs in a browser window",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "action": {

  },
  "manifest_version": 3,
  "commands": {
    "reload-all-tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space"
      },
      "description": "Reloads all tabs"
    }
  }
}